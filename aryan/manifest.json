
{
    "manifest_version": 3,
    "name": "Youtube video summarizer",
    "description":"An extension to summarize youtube video",
    "version": "1.0",
    "permissions": ["activeTab","declarativeContent"],
    "host_permissions":["https://127.0.0.1:5000/*"],
    
    
    "action": {
        "default_title": "Summarize this video",
        "default_icon":{
            "16":"logo.png",
            "32":"logo.png",
            "48":"logo.png",
            "128":"logo.png"
        },
        "default_popup":"popup.html"
    },
    "icons": {
        "16":"logo.png",
        "32":"logo.png",
        "48":"logo.png",
        "128":"logo.png"
    }
   
}
